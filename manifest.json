{
    "manifest_version": 2,
    "name": "Awin",
    "version": "1.0",
  
    "description": "Awin Extension",
     "icons":{
        "48": "Icon/awin-icona.png"
     },
    "permissions": [
        "activeTab",
        "tabs",
        "webRequest",
        "downloads",
        "cookies",
        "clipboardWrite",
        "clipboardRead",
        "webRequestBlocking",
        "<all_urls>"
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "browser_action": {
        "default_icon": "Icon/awin-icona.png",
        "default_title": "Awin Extension",
        "default_popup": "popup/tab.html"
      },
      "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["DWIN.js"]
        }
      ],
      "commands": {
        "Shortcut1": {
          "suggested_key": {
            "default": "Ctrl+1"
          }
        },
        "Shortcut2": {
          "suggested_key": {
            "default": "Ctrl+2"
          }
        },
        "Shortcut3": {
          "suggested_key": {
            "default": "Ctrl+3"
          }
        },
        "Shortcut4": {
          "suggested_key": {
            "default": "Ctrl+4"
          }
        },
        "Shortcut5": {
          "suggested_key": {
            "default": "Ctrl+5"
          }
        },
        "Shortcut6": {
          "suggested_key": {
            "default": "Ctrl+6"
          }
        },
        "Shortcut7": {
          "suggested_key": {
            "default": "Ctrl+7"
          }
        },
        "Shortcut8": {
          "suggested_key": {
            "default": "Ctrl+8"
          }
        },
        "Shortcut9": {
          "suggested_key": {
            "default": "Ctrl+9"
          }
        },
      } 
  }

